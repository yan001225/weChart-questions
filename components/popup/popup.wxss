.popup {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0,0,0,0.5);
  z-index: 999;
  padding: 0 30rpx;
}
.contianer {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 30rpx;
  right: 30rpx;
  /* height: 200rpx; */
  background-color: #fff;
  border-radius: 15rpx;
}
.title {
  height: 80rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  background-color: #272A3B;
  color: #78BD02;
  font-size: 40rpx;
  line-height: 80rpx;
  text-align: center;
}
.list {
  padding: 25rpx 0;
  display: flex;
  align-items: center;
  /* justify-content: space-around; */
  flex-wrap: wrap;
}
.list-item {
  width: 170rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  margin: 20rpx 0;
}
.img-view {
  width: 90rpx;
  height: 90rpx;
  border-radius: 26rpx;
  background-color: #78BD02;
}
.img {
  width: 90rpx;
  height: 90rpx;
}
